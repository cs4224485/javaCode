<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <listener>
        <listener-class>com.harry.listener.MyRequestListener</listener-class>
    </listener>
    <listener>
        <listener-class>com.harry.listener.MyRequestAttributeListener</listener-class>
    </listener>
    <listener>
        <listener-class>com.harry.listener.Student</listener-class>
    </listener>
    <listener>
        <listener-class>com.harry.listener.onlineUser.MyRequestListener</listener-class>
    </listener>
    <listener>
        <listener-class>com.harry.listener.onlineUser.MyServletContexLinstener</listener-class>
    </listener>
    <listener>
        <listener-class>com.harry.listener.onlineUser.MySessionListenner</listener-class>
    </listener>
    <listener>
        <listener-class>com.harry.listener.offlineUser.MyServletContestListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>com.harry.listener.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>com.harry.listener.servlet.DeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/deleteUser</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.harry.listener.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>